alias ls='ls -GFh'

# git
alias g='git'
alias s='git status -s'
alias ga='git add --all .'
alias gs='git status'
alias gpl='git pull'
alias gpr='git pull --rebase'
alias gsq='git add . && git commit --amend --no-edit'
alias gl='git log'
alias gca='git commit -a'
alias gcm='git commit'
alias gm='git commit -a -m'

# ruby stuff
alias be='bundle exec'
